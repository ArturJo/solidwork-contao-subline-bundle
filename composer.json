{
  "name": "solidwork/contao-subline-bundle",
  "description": "Fügt dem Überschriften-Element eine Subline hinzu.",
  "type": "contao-bundle",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "Solidwork\\ContaoSublineBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Solidwork\\ContaoSublineBundle\\ContaoManager\\Plugin"
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true
    }
  }
}
